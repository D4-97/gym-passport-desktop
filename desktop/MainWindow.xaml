        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gym_Passport"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Center" TextWrapping="Wrap" Text="Usuario" VerticalAlignment="Top" Width="142" Margin="0,189,0,0" Height="27" FontSize="14"/>
        <TextBox HorizontalAlignment="Center" Margin="0,223,0,0" TextWrapping="Wrap" Text="Contraseña" VerticalAlignment="Top" Width="142" Height="27" FontSize="14"/>
        <Button Content="Iniciar sesión" HorizontalAlignment="Center" Margin="0,284,0,0" VerticalAlignment="Top" Height="32" Width="104" FontSize="16"/>
        <Label Content="BIENVENIDO" HorizontalAlignment="Center" Margin="0,85,0,0" VerticalAlignment="Top" FontSize="30"/>

    </Grid>
</Window>
